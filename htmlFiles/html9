		addSpotify: function() {
			$('<a href="https://open.spotify.com/user/brooklynvegan" title="Visit us on Spotify" class="share_link spotify" target="_blank"><span class="social_icon">Follow on Spotify</span></a>').insertAfter(instagram_nav);
		}
	};

	$(document).ready(function(){
		socialNav.init();
	});
})(jQuery);

