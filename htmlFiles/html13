<script> 

var EsriAnalytics = (function () {
    
    var api = {};

    api.analyticsInit = function () {
        if (window.location.hostname) {
            window.digitalData = {
                timestamp: new Date(),
                pageBottomCalled: false
            };
            window.addEventListener("message", receiveMessage, false);
            api.collectData();
        }
        $(window).on('focusin', function () {
            clearJobQueue();
        });
    };

    api.recordMetrics = function (m) {
        if (digitalData) {
            for (var p in m) {
                digitalData[p] = m[p];
            }
        }
        //DEBUG CODE
        // if (typeof ANALYTICS_DEBUG !== 'undefined' && ANALYTICS_DEBUG && p.indexOf('scroll') === -1) {
        //     logMetric(p, m, ">");
        // }
    };

    /*
     * Page-load metrics
     */

    api.collectData = function () {
        collectPageInfo();
        collectSessionData();
        collectScrollPosition();
        collectEsriMetaData();
        collectCampaignData();
    };

    function collectPageInfo() {
        var pathArray = window.location.pathname.split('/'),
            host_name = (window.location.hostname.indexOf(window.location.protocol) !== -1)
                ? window.location.hostname.substr(window.location.protocol.length)
                : window.location.hostname;

        api.recordMetrics({
            pageInfo: {
                siteSection1: host_name,
                siteSection2: pathArray.length > 1 ? pathArray[1] : '',
                siteSection3: pathArray.length > 2 ? pathArray[2] : '',
                siteSection4: pathArray.length > 3 ? pathArray[3] : '',
                url: window.location.href,
                pageName: getPageName()
            }
        });
    }

    function collectSessionData() {
        //Detect Browser Language
        api.recordMetrics({
            language: {
                current: navigator.language,
                acceptedLanguages: navigator.languages
            },
            referrer: document.referrer
        });
    }

    function collectScrollPosition() {
        if (document.body.scrollHeight >= window.innerHeight) {
            digitalData.scroll_y = 0;
            $(window).scroll(function () {
                var scroll_y = Math.floor(window.scrollY / (document.body.scrollHeight - window.innerHeight) * 100);
                if (scroll_y > digitalData.scroll_y) {
                    api.recordMetrics({'scroll_y': scroll_y});
                }
            });
        } else {
            digitalData.scroll_y = 100;
        }
    }

    function collectEsriMetaData() {
        var vals = {},
            metas = document.getElementsByTagName('meta');

        for (var i = 0; i < metas.length; i++) {
            var metaName = metas[i].getAttribute("name");
            if (stringStartsWith(metaName, 'esri-')) {
                vals[metaName.substring(5)] = metas[i].getAttribute("content");
            }
        }
        api.recordMetrics({meta: vals});
    }

    function collectCampaignData() {
        //Record campaign values
        var el = $('[data-campaign-id]'),
            page_campaign = el.length ? el[0].data('campaign-id') : '';
        api.recordMetrics({'campaign': page_campaign});
    }

    var stringStartsWith = function (string, prefix) {
        if (string) {
            return string.slice(0, prefix.length) == prefix;
        }
    };

    var getPageName = function () {

        var page_name = '';

        if (window.location.href.substr(0, 7) === "http://") {
            page_name = window.location.href.substr(7);
        } else if (window.location.href.substr(0, 8) === "https://") {
            page_name = window.location.href.substr(8);
        } else if (window.location.href.substr(0, 2) === "//") {
            page_name = window.location.href.substr(2);
        } else {
            page_name = (window.location.href.indexOf('//'))
        }

        var page_name_array = page_name.split('/').filter(function (page_name_part) {
            if (page_name_part) {
                return page_name_part;
            }
        });

        return page_name_array.slice(0, 4).join(': ');
    };
    /*
     * Metric-tracking job & queuing functions
     */

    api.dataQueue = {};
    api.jobQueue = {};

    var receiveMessage = function (event) {
        var origin = event.origin || event.originalEvent.origin; // For Chrome, the origin property is in the event.originalEvent object.
        // if (origin === message_origin) {
            var message = (typeof event.data === 'object') ? event.data : JSON.parse(event.data);
            if (typeof message === 'object' && message.message_type === 'pFM') {
                api.managePformMetrics(message);
            }
        // }
    };

    api.managePformMetrics = function (payload) {
        if(!digitalData){return;}
        var data = JSON.parse(payload.data);
        switch (payload.rule) {
            case 'pFormStart':
                api.dataQueue['pFormMetrics'] = data;
                queueJob('pFormStart');
                runQueuedJob('pFormStart');
                break;
            case 'pFormData':
                switch (payload.action) {
                    case 'init':
                        if (!digitalData.pageBottomCalled) {
                            if(!digitalData.hasOwnProperty('pFormMetrics')){
                                digitalData.pFormMetrics = {};
                            }
                            digitalData.pFormMetrics.timeStamp = data.timeStamp;
                            digitalData.pFormMetrics.sessionId = data.sessionId;
                            digitalData.pFormMetrics.formId = data.formId;
                            digitalData.pFormMetrics.inputCount = data.inputCount;
                            digitalData.pFormMetrics.inputsCompleted = 0;
                            digitalData.pFormMetrics.lastInputId = '';
                            digitalData.pFormMetrics.lastInputIndex = 0;
                            digitalData.pFormMetrics.lastInputName = '';
                            digitalData.pFormMetrics.lastInputType = '';
                            digitalData.pFormMetrics.errorMessages = data.errorMessages;
                        } else {
                            api.dataQueue['pFormMetrics'] = data;
                            queueJob('pFormLoad');
                            runQueuedJob('pFormLoad');
                        }
                        break;
                    case 'error': EsriAnalytics.dataQueue.hasOwnProperty('pFormMetrics')
                        var session_data = EsriAnalytics.dataQueue.hasOwnProperty('pFormMetrics')
                            ? EsriAnalytics.dataQueue.pFormMetrics
                            : digitalData.pFormMetrics;

                        data.lastInputId = session_data.lastInputId;
                        data.lastInputIndex = session_data.lastInputIndex;
                        data.lastInputName = session_data.lastInputName;
                        data.lastInputType = session_data.lastInputType;
                        data.timeStamp = session_data.timeStamp;
                        data.sessionId = session_data.sessionId;
                        api.dataQueue['pFormMetrics'] = data;
                        queueJob('pFormError');
                        runQueuedJob('pFormError');
                        break;
                    case 'queue':
                        api.dataQueue['pFormMetrics'] = data;
                        queueJob('pFormData');
                        break;
                    case 'run':
                        api.dataQueue['pFormMetrics'] = data;
                        queueJob('pFormData');
                        runQueuedJob('pFormData');
                        break;
                }
                break;
            case 'pFormEnd':
                if(api.dataQueue['pFormMetrics']) {
                    api.dataQueue['pFormMetrics'].formCompleted = true;
                }
                queueJob('pFormEnd');
                runQueuedJob('pFormEnd');
                break;
        }
    };

    var jobFunctions = {
        pFormLoad: function () {
            if (typeof digitalData !== 'undefined') {
                digitalData.pFormMetrics = api.dataQueue['pFormMetrics'];
                _satellite.track('IFORMLOAD');
            }
        },
        pFormStart: function () {
            if (typeof digitalData !== 'undefined') {
                digitalData.pFormMetrics = api.dataQueue['pFormMetrics'];
                _satellite.track('IFORMSTART');
            }
        },
        pFormData: function () {
            if (typeof digitalData !== 'undefined') {
                digitalData.pFormMetrics = api.dataQueue['pFormMetrics'];
                _satellite.track('IFORMDATA');
            }
        },
        pFormEnd: function () {
            if (typeof digitalData !== 'undefined') {
                digitalData.pFormMetrics = api.dataQueue['pFormMetrics'];
                _satellite.track('IFORMEND');
            }
        },
        pFormError: function(){
            if (typeof digitalData !== 'undefined') {
                digitalData.pFormMetrics = api.dataQueue['pFormMetrics'];
                _satellite.track('IFORMERROR');
            }
        }
    };

    var queueJob = function (job) {
        api.jobQueue[job] = {"func": job};
    };

    var removeJob = function (job) {
        if (api.jobQueue.hasOwnProperty(job)) {
            delete api.jobQueue[job];
        }
    };

    var runQueuedJob = function (job) {
        if (api.jobQueue.hasOwnProperty(job)) {
            jobFunctions[job]();
            removeJob(job);
        }
    };

    var clearJobQueue = function () {
        var queue = api.jobQueue;
        for (var job in queue) {
            if(queue.hasOwnProperty(job)){
                runQueuedJob(job);
            }
        }
    };

    return api;
})();
</script>

