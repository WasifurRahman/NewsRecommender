	// Top 89 Ballot Javascript
	$(document).ready(function () {
	    $("#song_list input[name='song[]']").change(function () {
	    	// console.log('checked a song');
	        var maxAllowed = 10;
	        var cnt = $("input[name='song[]']:checked").length;
	        if (cnt > maxAllowed) {
	            $(this).prop("checked", "");

	            alert('You can select a maximum of ' + maxAllowed + ' songs. \n\nDe-select individual songs to make replacement selections on your ballot.');
	            // alert('You can select a maximum of ' + maxAllowed + ' songs.');
	        } else {
	        	$(this).parent().toggleClass("selected");
	        	$("#select_message .status").text("You have " + (maxAllowed-cnt) + " remaining.");
	        }
	    });

	    $( "#ballot" ).submit(function( event ) {
	    	var cnt = $("input[name='song[]']:checked").length;
	    	if( cnt == 0) {
				alert( "No songs have been selected." );
				event.preventDefault();
	    	}
		});

	});
	// Top 89 Login Field Focus
	$("a.playlist_login_link").click(function( event ) {
		$("#loginBlock input[name=user_email]:first").focus();
		event.preventDefault();
	});
	</script> </body> </html>