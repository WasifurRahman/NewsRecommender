        <meta itemprop="dateModified" content="2016-06-11T10:54:02+00:00" />
        <meta itemprop="datePublished" content="2016-06-11T10:54:02+00:00" />
        <meta itemprop="headline" content="Chuwi HiBook Review: A tablet that can kind of double as an ultrabook" />

        <div itemprop="publisher" itemscope itemtype="https://schema.org/Organization">
            <div itemprop="logo" itemscope itemtype="https://schema.org/ImageObject">
                <meta itemprop="url" content="http://www.neowin.net/images/orion/icons/favicon-196x196.png">
                <meta itemprop="width" content="196">
