<script>
// Initialize Google Analytics tracker
// Define tracking functions
TOUT_TRACKING = {
  tout_played: function(event_name, properties) {
    ga('send', 'event', 'Tout_Videos', 'Play', properties.tout_uid);
    //When a Tout video content started to play
  },
  tout_played_first_quartile: function(event_name, properties) {
    ga('send', 'event', 'Tout_Videos', 'Play 25%', properties.tout_uid);
    //When a Tout video content progressed into 25% of it's total duration,
    //either user fast forward or naturally play through
  },
  tout_played_mid_point: function(event_name, properties) {
    ga('send', 'event', 'Tout_Videos', 'Play 50%', properties.tout_uid);
    //When a Tout video content progressed into 50% of it's total duration,
    //either user fast forward or naturally play through
  },
  tout_played_third_quartile: function(event_name, properties) {
    ga('send', 'event', 'Tout_Videos', 'Play 75%', properties.tout_uid);
    //When a Tout video content progressed into 75% of it's total duration,
    //either user fast forward or naturally play through
  },
  tout_played_complete: function(event_name, properties) {
    ga('send', 'event', 'Tout_Videos', 'Play 100%', properties.tout_uid);
    //either user fast forward or naturally play through
    //Your code goes here;
  },
  ad_requested: function(event_name, properties) {
    ga('send', 'event', 'Tout_Videos', 'Ad Requested', properties.tout_uid);
    //When Tout player made an outbond request to the ads network in attempt to get an ad.
    //This does NOT imply the the server will response with a ad or not.
  },
  ad_loaded: function(event_name, properties) {
    ga('send', 'event', 'Tout_Videos', 'Ad Network Responded', properties.tout_uid);
    //When the ads network responsed succesfully with anything, may be with/without any ads.
    //Sometimes ads network response with empty ads documents, this event stillfired. 
  },
  ad_ready: function(event_name, properties) {
   ga('send', 'event', 'Tout_Videos', 'Ad Ready', properties.tout_uid);
    //When the ads network resolved an ad and suggest the player to serve that ad.
    //This does NOT guarantee the ad will be played.
    //Since the actual ad creative may or may not be playable.
  },
  ad_started: function(event_name, properties) {
    ga('send', 'event', 'Tout_Videos', 'Ad Start', properties.tout_uid);
    //When Tout player started to play the ad, because the ad is playable
  },
  ad_completed: function(event_name, properties) {
    ga('send', 'event', 'Tout_Videos', 'Ad Completed', properties.tout_uid);
  },
  ad_error: function(event_name, properties) {
    ga('send', 'event', 'Tout_Videos', 'Ad Error', properties.tout_uid);
    //When anything prevent an ad from playing, can be but not limited to any of these:
    //* failed to communicate to the ads network for any reason
    //* ads network responsed with an empty response, no ads
    //* ads network responsed with invalid/corrupted data
    //* the ad content responsed from the ads network is not playable
    //* the player crashed for any reason
  },
  product_initialized: function(event_name, properties) {
    ga('send', 'event', 'Tout_Videos', 'Player Initialized', properties.tout_uid);
    //When the player initialization is completed and it's ready to go
  }
}
</script>

