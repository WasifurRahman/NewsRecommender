	$(window).scroll(function() {    
		var scroll = $(window).scrollTop();
		var divPosid = $('#btmsponsoredcontent');
		if(scroll > lastScrollPos){
			if (!divPosid.length) {
				return;
			}
			if(scroll >= ($('#btmsponsoredcontent').position().top - 100) && footerPosCheck == false){
				openMoreLocalNewsbox();
				footerPosCheck = true;
			}
		}
		if(scroll < lastScrollPos){
			if(scroll <= ($('#loadComment').position().top - 250) && footerPosCheck == true){
				closeMoreLocalNewsbox('scroll');
			}
		}
		lastScrollPos = scroll
	});
